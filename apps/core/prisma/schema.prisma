generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  createdAt DateTime @default(now())
}

model Page {
  id        Int      @id @default(autoincrement())
  url       String   @unique
  createdAt DateTime @default(now())
}